<% provide(:title, 'Experiments') %>
<h3>Experimental Data</h3>
   <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="experiment_data">
    <thead>                         
      <tr>
        <th>Exp. no.</th>                          
        <th>Sys.ID</th>
        <th>logFC</th>
        <th>logCPM</th>
        <th>Pval</th>
        <th>FDR</th>
        <th>Name</th>
        <th>AA</th>
        <th>Similarity</th>
        <th>Product</th>
        
      </tr>
    </thead>
    <tbody>
      <% @experiment_data.each do |experiment_data| %>       
      <tr>
        <td><%= experiment_data.exp %></td>
        <td><%= experiment_data.sys_id %></td>  
        <td><%= experiment_data.logFC.round(4) %></td>
        <td><%= experiment_data.logCPM.round(4) %></td>
        <td><%= experiment_data.Pval.round(4) %></td>
        <td><%= experiment_data.FDR.round(4) %></td>
        <td><%= experiment_data.name%></td> 
        <td><%= experiment_data.AA %></td>    
        <td><%= experiment_data.sim %></td>
        <td><%= experiment_data.prod %></td>
        
      </tr>
    <% end %>
  </tbody>
</table> 

  

